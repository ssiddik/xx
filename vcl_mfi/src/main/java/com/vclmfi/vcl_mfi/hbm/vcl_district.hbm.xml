<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
<class name="com.vclmfi.vcl_mfi.pojo.vcl_district" table="vcl_district">

	<id name="districtId" type="integer" column="district_id">
        <generator class="increment"/>
    </id>
    <property name="districtShortCode" column="district_short_cd" type="string"/>
    <property name="districtName" column="district_name" type="string"/>
    <property name="stateId" column="state_id" type="integer"/>
     
</class>
</hibernate-mapping>